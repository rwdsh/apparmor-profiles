abi <abi/3.0>,

include <tunables/global>

/usr/bin/swaybar {
  include <abstractions/base>
  include <abstractions/fonts>

  /usr/bin/{bash,sh,zsh,dash} Ux,
  /usr/bin/swaybar mr,

  /usr/share/icons/{,**} r,
  /usr/share/pixmaps/{,**} r,

  owner /dev/shm/sway-* rw,
  owner @{HOME}/.cache/fontconfig/** rw,

}
